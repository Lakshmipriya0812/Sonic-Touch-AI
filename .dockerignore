mongodb-backups/
mongodb_data/

# Ignore local environment files (.env, etc.)
.env

# Ignore VS Code workspace settings
.vscode/

# Ignore temporary files from your editor (e.g., Vim, Emacs, etc.)
*~ 
*.swp

# Ignore .git directory
.git
.gitignore
